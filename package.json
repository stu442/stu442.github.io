{
  "private": true,
  "name": "gjg-blog",
  "description": "A technical blog covering a wide range of interesting and diverse topics. It mainly covers front-end and productivity.",
  "version": "1.3.16",
  "author": "GJG",
  "license": "0BSD",
  "starter-name": "gatsby-starter-minimal-blog",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "develop:cypress": "cross-env CYPRESS_SUPPORT=y yarn develop",
    "build:cypress": "cross-env CYPRESS_SUPPORT=y yarn build",
    "start": "gatsby clean && gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "deploy": "gatsby build && gh-pages -d public -b deploy"
  },
  "dependencies": {
    "@lekoarts/gatsby-theme-minimal-blog": "^6.2.1",
    "@svgr/webpack": "^8.1.0",
    "framer-motion": "^11.2.3",
    "gatsby": "^5.13.3",
    "gatsby-plugin-feed": "^5.13.1",
    "gatsby-plugin-manifest": "^5.13.1",
    "gatsby-plugin-sharp": "^5.13.1",
    "gatsby-plugin-sitemap": "^6.13.1",
    "gatsby-plugin-svgr": "^3.0.0-beta.0",
    "gatsby-plugin-webpack-statoscope": "^1.0.3",
    "gatsby-remark-copy-linked-files": "^6.13.1",
    "gatsby-remark-images": "^7.13.1",
    "gatsby-transformer-remark": "^6.13.1",
    "gh-pages": "^6.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.19.13",
    "@types/react": "^18.2.51",
    "@types/react-dom": "^18.2.18",
    "cross-env": "^7.0.3",
    "typescript": "^5.3.3"
  }
}
